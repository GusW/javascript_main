{
  "manifest_version": 2,

  "name": "Reality Show blocker",
  "description": "Hides some Brazilian reality shows spam",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "main.html"
  },

  "content_scripts": [
    {
      "js": [ "jquery-3.1.1.min.js", "main.js" ],
      "matches": [ "http://*/*", "https://*/*"]
    }
  ]
}
